<head>
<script src="//maps.google.com/maps/api/js?v=3.13&sensor=false&libraries=geometry" type="text/javascript"></script>
<script src="//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js" type="text/javascript"></script>
<%= javascript_include_tag "application" %>
<script>
  var hash = <%= raw @hash.to_json %>;
</script>
</head>

<div class='row'>
<h2>Available Swarms</h2>
<% @swarms.each do |swarm| %>
  <% unless swarm.assigned %>
    <div class="panel">
      <%= swarm.name %>
      Swarm active?: <%= swarm.active_status %>
       Assigned?: <%= swarm.assigned %>
      <%= button_to "CLAIM", {controller: "swarms", action: "assign_swarm", id: swarm.id }, class: "button alert" %>
    </div>
  <% end %>
<% end %> 
<h2> My Swarms</h2>
<% if @userswarms %>
<% @userswarms.each do |swarm| %>
  <div class="panel">
    <%= swarm.name %>
    Swarm active?: <%= swarm.active_status %>
     Assigned?: <%= swarm.assigned %>
  </div>
<% end %> 
<% else %>
You have no swarms!
<% end %>
</div>

<%= button_to "Logout", {controller: "sessions", action: "destroy"}, class: "button alert" %>

<div style='width: 800px;'>
  <div id="map" style='width: 800px; height: 400px;'></div>
</div>
